{
    "title": "マイルール",
    "rules": [
        {
            "description": "hを左矢印に変更",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        },
                        "key_code": "h"
                    },
                    "to": [
                        {
                            "repeat": true,
                            "key_code": "left_arrow"
                        }
                    ]
                }
            ]
        },
        {
